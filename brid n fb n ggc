/interface bridge
add name=bridge-top-kedung
/interface vlan
add interface=ether11 name=vlan701top vlan-id=701
add interface=ether13 name=vlan701topngalor vlan-id=701
/interface bridge port
add bridge=bridge-top-kedung interface=vlan701top
add bridge=bridge-top-kedung interface=vlan701topngalor

